{
  "expo": {
    "name": "pediFacil parceiros",
    "slug": "pedifacil-parceiros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-black.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pedifacil.deliveryparceiros",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-black.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pedifacil.deliveryparceiros",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "NOTIFICATIONS"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#FFA500"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "targetSdkVersion": 35,
            "compileSdkVersion": 35
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notifications.png",
          "androidMode": "default",
          "androidCollapsedTitle": "pediFacil Parceiros",
          "iosDisplayInForeground": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "64a25875-5d0c-4f66-b987-30bafb5fec21"
      }
    }
  }
}
